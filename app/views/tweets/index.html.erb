
  <% @tweets.each do |t| %>
    <div class="text">
       
       <div class="kakutoko"><%=safe_join(t.text.split("\n"),tag(:br))%></div>
       
       <div class="osutoko">
          <p><%= link_to "edit", edit_tweet_path(t.id), class: 'osu' %></p>
          <p><%= link_to "delete", "/tweets/#{t.id}", method: :delete, class: 'osu' %></p>
       </div>
    
    </div>
  <% end %>
   <%= paginate @tweets %>